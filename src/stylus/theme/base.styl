@import './animations.styl'
@import './article.styl'
@import url('https://fonts.googleapis.com/css?family=Quattrocento|Lora|Nanum+Myeongjo')

*
  text-rendering optimizeLegibility
  font-variant-ligatures common-ligatures
  outline none

*, :after, :before
  box-sizing border-box

body
  font-smoothing antialiased
  background-color #f7f9fb
  color #174f7c
  font-size 16px
  font-weight 400
  line-height 26px
